%%% Flag phd@afterintro
\newif\if@phd@afterintro\@phd@afterintrofalse
\newcommand*{\preface}[1][\prefacename]{%
\if@twocolumn
\@restonecoltrue\onecolumn
\else
\@restonecolfalse
\fi
\chapter*{#1
\chaptermark{#1}\sectionmark{#1}}%
\ifnum \c@secnumdepth >\m@ne
\if@mainmatter
\ClassWarningNoLine{PhdThesis}{preface environment %
should NOT be used in mainmatter mode}
\fi
\typeout{#1.}%
\addcontentsline{toc}{chapter}
{\protect\numberline{}#1}%
\if@phd@afterintro\else
\global\@phd@afterintrotrue
\global\let\@phd@thechapter\thechapter
\fi
\gdef\thechapter{P}
\setcounter{section}{0}%
\setcounter{subsection}{0}%
\else
\addcontentsline{toc}{chapter}{#1}%
\fi
\if@restonecol\twocolumn\fi
\thispagestyle{empty}
}
\newcommand*{\introduction}[1][\introductionname]{%
\setcounter{section}{0}%
\setcounter{subsection}{0}%
\if@twocolumn
\@restonecoltrue\onecolumn
\else
\@restonecolfalse
\fi
\chapter*{#1
\chaptermark{#1}\sectionmark{#1}}%
\ifnum \c@secnumdepth >\m@ne
\if@mainmatter
\ClassWarningNoLine{PhdThesis}{introduction environment %
should NOT be used in mainmatter mode}
\fi
\typeout{#1.}%
\addcontentsline{toc}{chapter}
{\protect\numberline{}#1}%
\if@phd@afterintro\else
\global\@phd@afterintrotrue
\global\let\@phd@thechapter\thechapter
\fi
\gdef\thechapter{I}
\setcounter{section}{0}%
\setcounter{subsection}{0}%
\else
\addcontentsline{toc}{chapter}{#1}%
\fi
\if@restonecol\twocolumn\fi
\thispagestyle{empty}
}
%%% Flag phd@afterapp
\newif\if@phd@afterapp\@phd@afterappfalse
\newcommand*{\conclusions}[1][\conclusionsname]{%
\if@twocolumn
\@restonecoltrue\onecolumn
\else
\@restonecolfalse
\fi
\chapter*{#1
\chaptermark{#1}\sectionmark{#1}}%
\ifnum \c@secnumdepth >\m@ne
